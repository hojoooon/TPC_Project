<!DOCTYPE html>
<html lang="en">
<head>
    <title><%= title %></title>
    <link href="/public/css/url.css" type="text/css" rel="stylesheet">
    <%- include('link') %>
</head>
<body>
    <%- include('topnav') %>
    <%- include('subnav') %>
    <div class="url_input">
        <form>
            <input type="url" name="rootUrl" placeholder="ex) https://www.ppomppu.co.kr" autocomplete="off" required>
            <div class="url_options">
                <div class="url_first">
                    <div class="inner_first">Browser Wait time</div>
                    <div class="inner_second">
                        <input name="time" type="number" step="0.1" required>
                    </div>
                    <div class="inner_third">sec</div>
                </div>
                <div class="url_second">
                    <div class="inner_fourth">Depth</div>
                    <div class="inner_fifth">
                        <input name="DEPTH" type="number" required>
                    </div>
                </div>
                <div class="url_third">
                    <div class="inner_sixth">Number of Cookies</div>
                    <div class="inner_seventh">
                        <input name="number" type="number" required>
                    </div>
                </div>
            </div>
            <div class="url_start_btn">
                <button type="submit">Start</button>
            </div>
        </form>
    </div>
</body>
</html>
